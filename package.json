{
  "name": "pokemmo",
  "main": "./public/src/es6/main.js",
  "version": "0.0.1",
  "logger": "trace",
  "description": "Open Pokemmo RPG world",
  "homepage": "https://github.com/Tsur/pokemmo-js",
  "private": true,
  "license": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tsur/pokemmo-js"
  },
  "bugs": "https://github.com/Tsur/pokemmo-js/issues",
  "engines": {
    "node": ">=4"
  },
  "dependencies": {},
  "devDependencies": {
    "babel": "^5.4.7",
    "babel-runtime": "^5.8.20",
    "babelify": "^7.1.0",
    "browserify": "^12.0.1",
    "jquery": "^2.1.4",
    "loglevel": "^1.4.0",
    "mongodb": "^2.0.47",
    "ramda": "^0.17.1",
    "socket.io": "^1.3.7",
    "uglify-js": "^2.5.0"
  },
  "scripts": {
    "start": "npm install",
    "lint": "eslint --ext .js server/es6",
    "database": "mongod --config /usr/local/etc/mongod.conf --dbpath ./dbpath",
    "server:es6": "babel-node --stage 0 server/es6/main.js",
    "server": "node dist/server.out.js",
    "build:server": "cd server/src && haxe -main pokemmo_s/Main.hx -lib nodejs -js ../../dist/server.out.js",
    "build:client": "browserify $npm_package_main --transform [babelify --stage 0] > dist/client.out.js && node_modules/uglify-js/bin/uglifyjs --compress --mangle -- dist/sdk.js > dist/client.out.min.js"
  },
  "config": {}
}
